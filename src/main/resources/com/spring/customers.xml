<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://www.springframework.org/schema/beans
  http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">

  <bean id="customer1" class="com.spring.Customer">
    <constructor-arg name="id" value="1"/>
    <constructor-arg name="name" value="Xu Yukun"/>
    <constructor-arg name="balance" value="1100"/>
  </bean>
  <bean id="customer2" class="com.spring.Customer">
    <constructor-arg name="id" value="2"/>
    <constructor-arg name="name" value="Chai Yew Meng"/>
    <constructor-arg name="balance" value="2200"/>
  </bean>
  <bean id="customer3" class="com.spring.Customer">
    <constructor-arg name="id" value="3"/>
    <constructor-arg name="name" value="Mohammed Arsat"/>
    <constructor-arg name="balance" value="3300"/>
  </bean>
  <bean id="customer4" class="com.spring.Customer">
    <constructor-arg name="id" value="4"/>
    <constructor-arg name="name" value="Vasikaran Thangavelu"/>
    <constructor-arg name="balance" value="4400"/>
  </bean>

</beans>
