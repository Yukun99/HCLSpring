<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://www.springframework.org/schema/beans
http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">

  <bean class="me.yukun.Parent" name="parent1">
    <constructor-arg type="int" value="34561"/>
    <constructor-arg type="java.lang.String" value="Rahuler"/>
  </bean>
  <bean class="me.yukun.Parent" name="parent2">
    <constructor-arg type="int" value="34562"/>
    <constructor-arg type="java.lang.String" value="Rahulia"/>
  </bean>

  <bean class="me.yukun.School" name="school">
    <constructor-arg type="int" value="1"/>
    <constructor-arg type="java.lang.String" value="Rahul Primary School"/>
  </bean>

  <!-- creating bean object using value attribute -->
  <bean class="me.yukun.Student" name="student">
    <constructor-arg type="int" value="3456"/>
    <constructor-arg type="java.lang.String" value="Rahul"/>
    <constructor-arg type="java.lang.String" value="Hyderabad"/>
    <constructor-arg type="java.util.List">
      <list>
        <ref bean="parent1"/>
        <ref bean="parent2"/>
      </list>
    </constructor-arg>
    <constructor-arg type="me.yukun.School" ref="school"/>
  </bean>
</beans>